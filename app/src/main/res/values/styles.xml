<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar"
        >
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/teal_700</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="Floating" parent="Widget.Design.FloatingActionButton">
        <item name="android:foreground">@drawable/floating_background
        </item>
    </style>

    <style name="FloatingPlus" parent="Widget.Design.FloatingActionButton">
        <item name="android:foreground">@drawable/floating_plus
        </item>

    </style>

    <style name="roundedImageViewRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10%</item>
    </style>

</resources>